{
    "name": "emptynick/voyager-bread",
    "description": "Extended BREAD functionality for Laravel Voyager",
    "license": "MIT",
    "homepage": "https://github.com/emptynick/voyager-bread",
    "support": {
        "issues": "https://github.com/emptynick/voyager-bread/issues",
        "source": "https://github.com/emptynick/voyager-bread"
    },
    "authors": [
        {
            "name": "Christoph Schweppe",
            "email": "info@cschweppe.de"
        }
    ],
    "require": {
        "larapack/voyager-hooks": "~1.2.1",
        "tcg/voyager": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Bread\\": "src/"
        }
    },
    "extra": {
        "hook": {
            "providers": [
                "Bread\\BreadServiceProvider"
            ],
            "aliases": {
                "Bread": "Bread\\BreadFacade"
            }
        }
    }
}
